From df2f1c7cada29f676e700cf3ab60f0444a07eea1 Mon Sep 17 00:00:00 2001
From: Ivan Oleynikov <ivan.oleynikov95@gmail.com>
Date: Fri, 6 Dec 2024 17:39:31 +0100
Subject: [PATCH] remove rk3588-hdmirx.dts overlay to prevent hdmirx activation

---
 arch/arm64/boot/dts/rockchip/overlay/Makefile        |  1 -
 .../boot/dts/rockchip/overlay/rk3588-hdmirx.dts      | 12 ------------
 2 files changed, 13 deletions(-)
 delete mode 100644 arch/arm64/boot/dts/rockchip/overlay/rk3588-hdmirx.dts

diff --git a/arch/arm64/boot/dts/rockchip/overlay/Makefile b/arch/arm64/boot/dts/rockchip/overlay/Makefile
index 31095455fc6e..e4e219ca615b 100644
--- a/arch/arm64/boot/dts/rockchip/overlay/Makefile
+++ b/arch/arm64/boot/dts/rockchip/overlay/Makefile
@@ -223,7 +223,6 @@ dtbo-$(CONFIG_ARCH_ROCKCHIP) += \
 	rk3588-uart8-m1.dtbo \
 	rk3588-w1-gpio3-b3.dtbo \
 	rk3588-w1-gpio4-b1.dtbo \
-	rk3588-hdmirx.dtbo \
 	rk3568-can0-m0.dtbo \
 	rk3568-can1-m0.dtbo \
 	rk3568-can1-m1.dtbo \
diff --git a/arch/arm64/boot/dts/rockchip/overlay/rk3588-hdmirx.dts b/arch/arm64/boot/dts/rockchip/overlay/rk3588-hdmirx.dts
deleted file mode 100644
index 91b3120d309e..000000000000
--- a/arch/arm64/boot/dts/rockchip/overlay/rk3588-hdmirx.dts
+++ /dev/null
@@ -1,12 +0,0 @@
-/dts-v1/;
-/plugin/;
-
-/ {
-	fragment@0 {
-		target = <&hdmirx_ctrler>;
-
-		__overlay__ {
-			status = "okay";
-		};
-	};
-};
-- 
